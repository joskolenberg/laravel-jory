{
  "name": "joskolenberg/laravel-jory",
  "description": "Create a flexible API for your Laravel application using json based queries.",
  "license": "MIT",
  "keywords": ["query", "laravel", "api", "jory", "json", "filter", "sort", "relation", "model", "resource"],
  "authors": [
    {
      "name": "Jos Kolenberg",
      "email": "jos@kolenberg.net"
    }
  ],
  "minimum-stability": "stable",
  "require": {
    "laravel/framework": "^5.8|^6.0",
    "joskolenberg/jory": "^2.0",
    "joskolenberg/eloquent-reflector": "^2.0",
    "rap2hpoutre/similar-text-finder": "^1.1"
  },
  "autoload": {
    "psr-4": {
      "JosKolenberg\\LaravelJory\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "JosKolenberg\\LaravelJory\\Tests\\": "tests/"
    }
  },
  "require-dev": {
    "orchestra/testbench": "^3.5|^4.0"
  },
  "extra": {
    "laravel": {
      "providers": [
        "JosKolenberg\\LaravelJory\\JoryServiceProvider"
      ]
    }
  }
}
